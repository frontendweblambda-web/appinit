{
	"name": "@appinit/agent-sdk",
	"version": "1.0.0",
	"description": "AI agent SDK for AppInit OS automation and integrations.",
	"private": true,
	"type": "module",
	"main": "./dist/cjs/index.cjs",
	"module": "./dist/esm/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"clean": "node ../../scripts/clean-dist.mjs dist",
		"prebuild": "pnpm clean",
		"build": "tsup",
		"dev": "tsup --watch",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"test": "vitest"
	},
	"dependencies": {
		"@appinit/types": "workspace:*",
		"@appinit/engine": "workspace:*",
		"@appinit/utils": "workspace:*",
		"deepmerge": "^4.3.1",
		"fs-extra": "^11.3.2",
		"tsup": "latest"
	},
	"devDependencies": {
		"typescript": "^5.9.3",
		"@types/node": "^20.11.30"
	},
	"author": "frontendweb-sg",
	"license": "MIT"
}
