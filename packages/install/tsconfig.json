{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"outDir": "dist",
		"rootDir": "src",
		"declaration": true,
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"target": "ES2022",
		"allowJs": false,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"strict": true
	},
	"include": ["src"]
}
