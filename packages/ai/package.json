{
	"name": "@appinit/ai",
	"version": "1.0.0",
	"description": "Appinit OS AI toolkit (LLM wrappers, agents, embeddings, prompts).",
	"private": true,
	"type": "module",
	"main": "./dist/cjs/index.cjs",
	"module": "./dist/esm/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"clean": "node ../../scripts/clean-dist.mjs dist",
		"prebuild": "pnpm clean",
		"build": "tsup",
		"dev": "tsup --watch",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"test": "vitest"
	},
	"dependencies": {
		"@appinit/types": "workspace:*",
		"@appinit/utils": "workspace:*",
		"@appinit/agent-sdk": "workspace:*",
		"openai": "^4.54.0",
		"ollama": "^0.4.1",
		"node-fetch": "^3.3.2"
	},
	"devDependencies": {
		"typescript": "^5.9.3",
		"@types/node": "^20.11.30",
		"rimraf": "^6.1.0",
		"tsup": "latest"
	},
	"engines": {
		"node": ">=18"
	},
	"author": "frontendweb-sg",
	"license": "MIT"
}
