{
	"name": "@appinit/utils",
	"version": "1.0.0",
	"description": "Shared utility helpers for Appinit OS.",
	"private": false,
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"clean": "node ../../scripts/clean-dist.mjs dist",
		"prebuild": "pnpm clean",
		"build": "tsup src/index.ts --format esm --dts --clean",
		"dev": "tsup src/index.ts --format esm --dts --watch",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"test": "vitest"
	},
	"dependencies": {
		"fs-extra": "latest",
		"@appinit/types": "workspace:*"
	},
	"devDependencies": {
		"tsup": "latest",
		"typescript": "latest",
		"vitest": "latest"
	},
	"publishConfig": {
		"access": "restricted"
	},
	"engines": {
		"node": ">=20.11.0"
	},
	"author": {
		"name": "Pradeep Kumar",
		"url": "https://github.com/frontendweblambda-web/appinit"
	},
	"license": "MIT"
}
