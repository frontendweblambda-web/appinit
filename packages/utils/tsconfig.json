{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"moduleResolution": "Bundler",

		"rootDir": "src",
		"outDir": "dist",
		"declaration": true,
		"declarationMap": false,

		"strict": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src"]
}
