{
	"name": "@appinit/template-resolver",
	"version": "0.1.0",
	"description": "Unified template resolver for Appinit OS (local, GitHub, URL, npm, marketplace).",
	"private": true,
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"clean": "node ../../scripts/clean-dist.mjs dist",
		"prebuild": "pnpm clean",
		"build": "tsup src/index.ts --format esm --dts --clean",
		"dev": "tsup src/index.ts --format esm --dts --watch --onSuccess \"echo [template-resolver rebuilt]\"",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"test": "vitest"
	},
	"dependencies": {
		"@appinit/types": "workspace:*",
		"@appinit/utils": "workspace:*",
		"@appinit/templates": "workspace:*",
		"@appinit/core": "workspace:*",
		"fs-extra": "^11.3.2"
	},
	"devDependencies": {
		"typescript": "^5.9.3",
		"vitest": "^1.0.0",
		"@types/node": "^20.11.30",
		"tsup": "latest",
		"glob": "latest"
	},
	"publishConfig": {
		"access": "restricted"
	},
	"engines": {
		"node": ">=20.11.0"
	},
	"author": {
		"name": "Pradeep Kumar",
		"url": "https://github.com/frontendweblambda-web/appinit"
	}
}
