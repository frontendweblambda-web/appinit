{
	"name": "@appinit/module-engine",
	"version": "1.0.0",
	"description": "Module engine for the AppInit ecosystem.",
	"private": false,
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"clean": "node ../../scripts/clean-dist.mjs dist",
		"prebuild": "pnpm clean",
		"build": "tsup src/index.ts --format esm,cjs --dts --clean",
		"dev": "tsup src/index.ts --format esm,cjs --dts --watch",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"test": "vitest"
	},
	"dependencies": {
		"@appinit/types": "workspace:*",
		"@appinit/utils": "workspace:*",
		"@appinit/core": "workspace:*",
		"chalk": "^5.6.2",
		"fs-extra": "^11.3.2",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"tsup": "latest",
		"typescript": "^5.9.3",
		"@types/node": "^20.11.30",
		"vitest": "^1.0.0"
	},
	"keywords": [
		"scaffold",
		"engine",
		"generator",
		"appinit",
		"typescript"
	],
	"license": "MIT",
	"publishConfig": {
		"access": "restricted"
	},
	"engines": {
		"node": ">=20.11.0"
	},
	"author": {
		"name": "Pradeep Kumar",
		"url": "https://github.com/frontendweblambda-web/appinit"
	}
}
