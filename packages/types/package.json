{
	"name": "@appinit/types",
	"version": "1.0.0",
	"description": "Shared TypeScript definitions for the Appinit ecosystem.",
	"private": false,
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"clean": "node ../../scripts/clean-dist.mjs dist",
		"prebuild": "pnpm clean",
		"build": "tsup src/index.ts --format esm,cjs --dts --clean",
		"dev": "tsup src/index.ts --format esm,cjs --dts --watch",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"test": "vitest"
	},
	"devDependencies": {
		"tsup": "latest",
		"typescript": "latest",
		"vitest": "latest"
	},
	"engines": {
		"node": ">=18"
	}
}
