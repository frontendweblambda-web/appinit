{
	"name": "@appinit/cli",
	"version": "1.0.0",
	"description": "The command-line interface for Appinit OS.",
	"private": false,
	"type": "module",
	"main": "./dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.js"
		}
	},
	"bin": {
		"appinit": "./bin/cli.js"
	},
	"files": [
		"bin",
		"dist"
	],
	"scripts": {
		"clean": "node ../../scripts/clean-dist.mjs dist",
		"prebuild": "pnpm clean",
		"build": "tsup src/index.ts --format esm --dts --clean",
		"dev": "tsup src/index.ts --format esm --dts --watch",
		"start": "node dist/index.js"
	},
	"dependencies": {
		"@appinit/config": "workspace:*",
		"@appinit/engine": "workspace:*",
		"@appinit/prompt": "workspace:*",
		"@appinit/types": "workspace:*",
		"@appinit/utils": "workspace:*",
		"@appinit/core": "workspace:*",
		"@appinit/template-resolver": "workspace:*",
		"@appinit/templates": "workspace:*",
		"chalk": "^5.6.2",
		"chalk-table": "^1.0.2",
		"dotenv": "^17.2.3",
		"ejs": "^3.1.10",
		"fs-extra": "^11.3.2",
		"globby": "^15.0.0",
		"json5": "^2.2.3",
		"openai": "^6.9.0",
		"semver": "^7.7.3",
		"simple-git": "^3.30.0"
	},
	"devDependencies": {
		"@types/node": "^20.19.25",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=20.11.0"
	},
	"author": {
		"name": "Pradeep Kumar",
		"url": "https://github.com/frontendweblambda-web/appinit"
	}
}
