{
	"name": "@appinit/config",
	"version": "1.0.0",
	"private": false,
	"type": "module",
	"description": "Official template & configuration collection for Appinit CLI.",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		}
	},
	"keywords": [
		"appinit",
		"template",
		"config",
		"scaffold"
	],
	"license": "MIT",
	"files": [
		"react",
		"next",
		"vue",
		"shared",
		"meta",
		"index.js",
		"dist"
	],
	"dependencies": {
		"@appinit/types": "workspace:*",
		"@appinit/utils": "workspace:*"
	},
	"devDependencies": {
		"fs-extra": "^11.3.2"
	},
	"scripts": {
		"clean": "node ../../scripts/clean-dist.mjs dist",
		"prebuild": "pnpm clean",
		"build": "tsup src/index.ts --format esm --dts --clean",
		"dev": "tsup src/index.ts --format esm --dts --watch",
		"start": "node dist/index.js"
	},
	"publishConfig": {
		"access": "restricted"
	},
	"engines": {
		"node": ">=20.11.0"
	},
	"author": {
		"name": "Pradeep Kumar",
		"url": "https://github.com/frontendweblambda-web/appinit"
	}
}
