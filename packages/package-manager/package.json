{
	"name": "@appinit/package-manager",
	"version": "0.1.1",
	"description": "Unified package manager abstractions for Appinit OS.",
	"private": true,
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"clean": "npx rimraf dist",
		"prebuild": "pnpm clean",
		"build": "pnpm clean && tsc -p tsconfig.json",
		"dev": "tsc -w -p tsconfig.json",
		"test": "vitest"
	},
	"engines": {
		"node": ">=18"
	},
	"dependencies": {
		"@appinit/types": "workspace:*",
		"@appinit/engine": "workspace:*"
	},
	"devDependencies": {
		"typescript": "^5.0.0",
		"vitest": "^1.0.0",
		"@types/node": "^20.11.30"
	}
}
