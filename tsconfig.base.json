{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"lib": ["ES2022", "DOM"],
		"types": ["node"],
		"jsx": "react-jsx",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"noEmit": true,
		"declaration": false,
		"emitDeclarationOnly": false,
		"baseUrl": ".",
		"paths": {
			"@appinit/*": ["packages/*/src", "packages/*/dist"]
		}
	}
}
